{
    "env": {
        "node": true,
        "es6": true,
        "jasmine": true,
        "mocha": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "eqeqeq": [
            2, "allow-null"
        ]
    },
    "parserOptions": {
        "ecmaVersion" : 6,
        "sourceType": "script"
    },
    "globals": {
        "should": true,
        "angular": true,
        "inject": true,
        "getJSONFixture": true
    }
}
